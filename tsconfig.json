{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "node",
    "strict": true,
    "lib": ["ES2020", "DOM"],
    "outDir": "./docs",
    "forceConsistentCasingInFileNames": true,
    "rootDir": "./src",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "downlevelIteration": false
  },
  "include": ["src"]
}